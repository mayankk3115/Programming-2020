import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in); 
		int k = 0;
		String s = sc.next();
		char n = sc.nextChar();
		/*String s = "my name maya";
		
		for(int i=0;i<s.length();i++){
		    if(s.charAt(i)=='a')
		    k++;
		}*/
		for(ch c : s.toCharArray()){
		    if(c == n)
		    k++;
		}
		
		System.out.println("Occurence of given character is:"+k);
	}
}
